{
    "manifest_version": 2,
    "version": "1.1",
    "name": "Whatsapp Wallpaper Changer",
    "description": "Extension for change your whatsapp wallpaper.",
    "author": "Di√≥genes Dietrich de Morais",
    "icons": {
                "16": "icon.png",
                "48": "icon.png",
                "128": "icon.png"
            },

    "permissions": [
        "storage"
      ],

    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "js": ["wallpaperWhatsapp.js"]

        }
    ]
}